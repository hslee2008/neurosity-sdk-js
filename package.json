{
  "name": "@neurosity/sdk",
  "version": "6.2.1-next.1",
  "description": "Neurosity SDK",
  "type": "module",
  "main": "./dist/index.m.js",
  "module": "./dist/index.m.js",
  "unpkg": "./dist/index.umd.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.m.js",
      "require": "./dist/index.cjs",
      "default": "./dist/index.modern.js",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "prebuild": "npm run clean",
    "build": "microbundle --tsconfig tsconfig.json",
    "build:docs": "npm run build:typedoc && cd website && npm install && npm run build",
    "build:typedoc": "del docs/reference && typedoc",
    "examples:node": "npm run build && node ./examples/node",
    "start": "tsc -w",
    "dev": "microbundle watch",
    "clean": "del dist",
    "prepublishOnly": "npm run build"
  },
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/neurosity/neurosity-sdk-js.git"
  },
  "keywords": [
    "neurosity",
    "api",
    "client",
    "neurosity"
  ],
  "engines": {
    "node": ">=4"
  },
  "author": "Alex Castillo",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/neurosity/neurosity-sdk-js/issues"
  },
  "homepage": "https://docs.neurosity.co",
  "dependencies": {
    "@neurosity/ipk": "^2.12.0",
    "axios": "^0.25.0",
    "fast-deep-equal": "^3.1.3",
    "firebase": "^8.10.0",
    "outliers": "0.0.3",
    "rxjs": "^7.5.6",
    "semver": "^7.3.8"
  },
  "devDependencies": {
    "@types/node": "^12.0.4",
    "@types/web-bluetooth": "^0.0.16",
    "copyfiles": "^2.3.0",
    "del-cli": "^1.1.0",
    "dotenv": "^6.0.0",
    "inquirer": "^6.0.0",
    "microbundle": "^0.15.1",
    "typedoc": "^0.17.4",
    "typedoc-plugin-internal-external": "^2.0.2",
    "typedoc-plugin-markdown": "^2.2.17",
    "typescript": "^4.8.3"
  }
}
